{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "server/**/*.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "/app/*",
  ],
  "exclude": [
    "node_modules"
  ]
}